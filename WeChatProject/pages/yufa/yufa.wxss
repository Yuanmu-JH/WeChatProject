/* pages/yufa/yufa.wxss */
.top{
  text-align: center;   /*文字居中*/
  line-height: 120rpx;
  color: rgb(27, 146, 156);
  background-color: rgb(170, 225, 230);
}

.goodList{
  width: 100%;
  /* 1.自动填充高度，2.超出部分隐藏 */
  overflow: hidden;
  background-color: lightblue;
}

.items{
  width: 350rpx;
  height: 400rpx;
  background-color: lightcyan;
  margin: 10rpx;
  /* 水平排列 */
  float: left;
  /* 设置字体 */
  text-align: center;
  font-size: 40rpx;
  font-weight: bolder;
  font-family: "微软雅黑";
  color: #000;
}

.items image{
  width: 350rpx;
  height: 300rpx;
}